ound: #ffffff;\r\n    padding: 2rem;\r\n    border-radius: 2px;\r\n}\r\n\r\n.card-single div:first-child span{\r\n    color: var(--color-primary);\r\n \r\n}\r\n\r\n.card-single div:last-child span{\r\n    font-size: 3rem; \r\n    color: var(--color-primary-dark);  \r\n}\r\n\r\n.card-single:last-child{\r\n    background: var(--color-primary-dark);    \r\n}\r\n\r\n.card-single:last-child h1,.card-single:last-child div:first-child span, .card-single:last-child div:last-child span {\r\n  color: var(--color-general);\r\n}\r\n\r\n.recent-grid{\r\n   padding-left: 20px;\r\n   padding-top: 2rem;\r\n   padding-right: 20px;\r\n   padding-bottom: 40px;\r\n   display: grid;\r\n   grid-gap: 2rem;\r\n   grid-template-columns: 70% auto;\r\n\r\n}\r\n\r\n.card{\r\n    background: var(--color-general);\r\n    border-radius: 5px;\r\n}\r\n\r\n.card-header, .card-body {\r\n    padding: 1rem;\r\n    margin-left: 0px;\r\n}\r\n\r\n.card-header{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n\r\n.card-header button{\r\n    background: var(--color-primary-dark);\r\n    border-radius: 10px;\r\n    color: var(--color-general);\r\n    font-size: .8rem;\r\n    padding: .5rem 1rem;\r\n    border: 1px solid var(--color-primary-dark);\r\n}\r\n\r\n.card-header button:hover{\r\n    background-color: #adf802;\r\n    color: black;\r\n    border: 1px solid var(--color-primary-dark);\r\n}\r\n\r\ntable{\r\n    border-collapse: collapse;\r\n}\r\n\r\nthead tr{\r\n    border-top: 1px solid #f0f0f0;\r\n    border-bottom: 1px solid #f0f0f0;\r\n}\r\n\r\nthead td{\r\n    font-weight: 700;\r\n}\r\n\r\ntd{\r\n    padding: .5rem 1rem;\r\n    font-size: .9rem;\r\n    color: #222;\r\n}\r\n\r\ntd .status{\r\n    height: 10px;\r\n    width: 10px;\r\n    border-radius: 50%;\r\n    margin-right: 1rem;\r\n}\r\n\r\ntr td:last-child{\r\n    display: flex;\r\n    align-items: center;   \r\n}\r\n\r\n.status.done{\r\n    background: rebeccapurple;\r\n    \r\n}\r\n\r\n.status.testing{\r\n    background: deepskyblue;\r\n}\r\n\r\n.status.process{\r\n    background: orangered;\r\n}\r\n\r\n.table-responsive{\r\n    width: 100%;\r\n    overflow-x: auto;\r\n}\r\n\r\n.customer{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: .5rem .7rem;\r\n}\r\n\r\n.info{\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.info img{\r\n    border-radius: 50%;\r\n    margin-right: .98rem;\r\n}\r\n\r\n.info h4{\r\n    font-size: .7rem;\r\n    font-weight: 600;\r\n    color: #222;\r\n}\r\n\r\n.info small{\r\n    font-weight: 200;\r\n    color: var(--color-greydarker);\r\n}\r\n\r\n.contact span{\r\n    font-size: .8rem;\r\n    display: inline-block;\r\n    margin-left: .1rem;\r\n    color: var(--color-primary);\r\n}\r\na {\r\n  text-decoration: none;\r\n  color: #adf802;\r\n}\r\n\r\n.header {\r\n  width: 100%;\r\n  height: var(--header-height);\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 0 1rem;\r\n  background-color: var(--color-general);\r\n  z-index: var(--z-fixed);\r\n  transition: 0.5s;\r\n}\r\n\r\n.header__toggle {\r\n  color: var(--color-primary-dark);\r\n  font-size: 1.5rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.header__img {\r\n  width: 50px;\r\n  height: 50px;\r\n  display: flex;\r\n  justify-content: center;\r\n  border-radius: 0%;\r\n  overflow: hidden;\r\n}\r\n\r\n.header__img img {\r\n  width: 40px;\r\n}\r\n\r\n.l-navbar {\r\n  position: fixed;\r\n  top: 0;\r\n  left: -30%;\r\n  width: var(--nav-width);\r\n  height: 100vh;\r\n  background-color: var(--color-primary-dark);\r\n  padding: 0.5rem 1rem 0 0;\r\n  transition: 0.5s;\r\n  z-index: var(--z-fixed);\r\n}\r\n\r\n.nav {\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  overflow: hidden;\r\n}\r\n\r\n.nav__logo,\r\n.nav__link {\r\n  display: grid;\r\n  grid-template-columns: max