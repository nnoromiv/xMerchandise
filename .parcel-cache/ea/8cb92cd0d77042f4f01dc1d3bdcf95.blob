;YACI,WAAW;QACf;IACJ;A;IAEA;QACI;YACI,WAAW;QACf;IACJ;AACJ","sources":["./styles/profile.css"],"sourcesContent":["@import url('https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,200;0,300;1,500&display=swap');\r\n\r\n\r\n:root {\r\n  --color-error: red;\r\n  --color-success: skyblue;\r\n  --font-family: sans-serif;\r\n  --color-skyblue: skyblue;\r\n  --color-primary: #adf802;\r\n  --color-primary-dark: #3e5801;\r\n  --color-grey: rgb(238, 238, 238);\r\n  --color-greydarker: #5a5a5a;\r\n  --color-general: #ffffff;\r\n  --header-height: 3rem;\r\n  --nav-width: 68px;\r\n  --body-font: 'Poppins', sans-serif;\r\n  --normal-font-size: 1rem;\r\n  --z-fixed: 100;\r\n}\r\n\r\n*{\r\n    padding: 0;\r\n    margin: 0;\r\n    box-sizing: border-box;\r\n}\r\n\r\n\r\n\r\na {\r\n  text-decoration: none;\r\n  color: #adf802;\r\n}\r\n\r\n.header {\r\n  width: 100%;\r\n  height: var(--header-height);\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 0 1rem;\r\n  background-color: var(--color-general);\r\n  z-index: var(--z-fixed);\r\n  transition: 0.5s;\r\n}\r\n\r\n.header__toggle {\r\n  color: var(--color-primary-dark);\r\n  font-size: 1.5rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.header__img {\r\n  width: 50px;\r\n  height: 50px;\r\n  display: flex;\r\n  justify-content: center;\r\n  border-radius: 0%;\r\n  overflow: hidden;\r\n}\r\n\r\n.header__img img {\r\n  width: 40px;\r\n}\r\n\r\n.l-navbar {\r\n  position: fixed;\r\n  top: 0;\r\n  left: -30%;\r\n  width: var(--nav-width);\r\n  height: 100vh;\r\n  background-color: var(--color-primary-dark);\r\n  padding: 0.5rem 1rem 0 0;\r\n  transition: 0.5s;\r\n  z-index: var(--z-fixed);\r\n}\r\n\r\n.nav {\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  overflow: hidden;\r\n}\r\n\r\n.nav__logo,\r\n.nav__link {\r\n  display: grid;\r\n  grid-template-columns: max-content max-content;\r\n  align-items: center;\r\n  column-gap: 1rem;\r\n  padding: 0.5rem 0 0.5rem 1.5rem;\r\n}\r\n\r\n.nav__logo {\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.nav__logo-icon {\r\n  font-size: 1.25rem;\r\n  color: var(--color-general);\r\n}\r\n\r\n.nav__logo-name {\r\n  color: #ffffff;\r\n  font-weight: 800;\r\n}\r\n\r\n.nav__link {\r\n  position: relative;\r\n  color: var(--color-primary);\r\n  margin-bottom: 1.5rem;\r\n  transition: 0.3s;\r\n}\r\n\r\n.nav__link:hover {\r\n  color: var(--color-general);\r\n}\r\n\r\n.nav__icon {\r\n  font-size: 1.25rem;\r\n}\r\n\r\n.show {\r\n  left: 0;\r\n}\r\n\r\n.body-pd {\r\n  padding-left: calc(var(--nav-width) + 1rem);\r\n}\r\n\r\n.active {\r\n  color: var(--color-general);\r\n}\r\n\r\n.active::before {\r\n  content: '';\r\n  position: absolute;\r\n  left: 0;\r\n  width: 2px;\r\n  height: 32px;\r\n  background-color: var(--color-general);\r\n}\r\n\r\n@media screen and (min-width: 768px) {\r\n  body {\r\n    margin: calc(var(--header-height) + 1rem) 0 0 0;\r\n    padding-left: calc(var(--nav-width) + 2rem);\r\n  }\r\n\r\n  .header {\r\n    height: calc(var(--header-height) + 1rem);\r\n    padding: 0 2rem 0 calc(var(--nav-width) + 2rem);\r\n  }\r\n  .header__img {\r\n    width: 40px;\r\n    height: 40px;\r\n  }\r\n  .header__img img {\r\n    width: 45px;\r\n  }\r\n  .l-navbar {\r\n    left: 0;\r\n    padding: 1rem 1rem 0 0;\r\n  }\r\n  .show {\r\n    width: calc(var(--nav-width) + 156px);\r\n  }\r\n  .body-pd {\r\n    padding-left: calc(var(--nav-width) + 188px);\r\n  }\r\n}\r\n\r\n\r\nbody{\r\n    background: #f0f0f0;\r\n    overflow-x: hidden;\r\n    padding-top: 90px;\r\n    font-family: 'Poppins', sans-serif;\r\n    margin: 0 100px;\r\n}\r\n\r\n.profile-header{\r\n    background: var(--color-general);\r\n    width: 100%;\r\n    display: flex;\r\n    height: 190px;\r\n    position: relative;\r\n    box-shadow: 0px 5px 10px rgba(0, 0, 0, .2);\r\n}\r\n\r\n.profile-img{\r\n    float: left;\r\n    width: 340px;\r\n    height: 200px;\r\n}\r\n\r\n.profile-img img{\r\n    border-radius: 50%;\r\n    height: 200px;\r\n    width: 200px;\r\n    border: 4px solid #fff;\r\n    box-shadow: 0px 5px 10px rgba(0, 0, 0, .2);\r\n    position: absolute;\r\n    left: 50px;\r\n    top: 50px;\r\n    z-index: 5;\r\n    background: var(--color-general);\r\n}\r\n\r\n.profile-nav-info{\r\n    float: left;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    padding-top: 60px;\r\n}\r\n\r\n.profile-nav-info h3{\r\n    font-variant: small-caps;\r\n    font-size: 2rem;\r\n    font-family: sans-serif;\r\n    font-weight: bold;\r\n}\r\n\r\n.profile-nav-info .address{\r\n    display: flex;\r\n    font-weight: bold;\r\n    color: #777;\r\n}\r\n\r\n.profile-nav-info .address p{\r\n    margin-right: 5px;\r\n}\r\n\r\n.profile-option{\r\n    width: 40px;\r\n    height: 40px;\r\n    background: var(--color-primary);\r\n    border-radius: 50%;\r\n    position: absolute;\r\n    right: 50px;\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    cursor: pointer;\r\n    transition: all .5s ease-in-out;\r\n    outline: none;\r\n}\r\n\r\n.profile-option .notification i{\r\n    color: var(--color-primary-dark);\r\n    font-size: 1.2rem;\r\n    transition: all 0.5s ease-in-out;\r\n}\r\n\r\n.profile-option .notification i:hover{\r\n    color: var(--color-greydarker);\r\n    transform: rotateY(180deg);\r\n}\r\n\r\n.profile-option:hover{\r\n    background: var(--color-general);\r\n}\r\n\r\n.profile-option .notification .alert-message{\r\n    position: absolute;\r\n    top: -5px;\r\n    right: -5px;\r\n    background: #fff;\r\n    color: var(--color-primary);\r\n    padding: 5px;\r\n    height: 20px;\r\n    width: 20px;\r\n    display: flex;\r\n    align-items: center;\r\n    border-radius: 50%;\r\n    justify-content: center;\r\n    font-size: .8rem;\r\n    font-weight: bold;\r\n}\r\n\r\n.main-bd{\r\n    width: 100%;\r\n    display: flex;\r\n    padding-right: 15px;\r\n}\r\n\r\n.profile-side{\r\n    width: 300px;\r\n    background: #fff;\r\n    box-shadow: 0px 3px 5px rgba(0, 0, 0, .2);\r\n    padding: 90px 30px 20px;\r\n    font-family: 'Bree serif', serif;\r\n    margin-left: 10px;\r\n    z-index: 99;\r\n}\r\n\r\n.profile-side p{\r\n    margin-bottom: 7px;\r\n    color: #3e5801;\r\n    font-size: 14px;\r\n}\r\n\r\n.profile-side p i{\r\n    color: #777;\r\n    margin-right: 10px;\r\n}\r\n\r\n.profile-btn{\r\n    display: flex;\r\n}\r\n\r\nbutton.chatbtn, button.createbtn {\r\n    border: 0;\r\n    padding: 10px;\r\n    width: 100%;\r\n    border-radius: 3px;\r\n    background: var(--color-primary);\r\n    font-family: 'Bree Serif' , sans-serif;\r\n    font-size: 1rem;\r\n    margin: 5px 2px;\r\n    cursor: pointer;\r\n    outline: none;\r\n    margin-bottom: 10px;\r\n    transition: background .3s ease-in-out;\r\n    box-shadow: 0px 5px 7px 0px rgba(0, 0, 0, .3);\r\n    color: #777;\r\n}\r\n\r\nbutton.chatbtn:hover, button.createbtn:hover {\r\n    background: var(--color-primary-dark);\r\n    color: #fff;\r\n}\r\n\r\nbutton.chatbtn i, button.createbtn i{\r\n    margin-right: 5px;\r\n}\r\n\r\n.user-rating{\r\n    display: flex;\r\n}\r\n\r\n.user-rating h3{\r\n    font-size: 2.5rem;\r\n    font-weight: 200;\r\n    margin-right: 5px;\r\n    letter-spacing: 1px;\r\n    color: var(--color-greydarker);\r\n}\r\n\r\n.user-rating .no-user{\r\n    font-size: .9rem;\r\n}\r\n\r\n.rate{\r\n    padding-top: 6px;\r\n}\r\n\r\n.rate i{\r\n    font-size: .9rem;\r\n    color: var(--color-primary-dark);\r\n}\r\n\r\n.l-nav{\r\n    width: 100%;\r\n    z-index: -1;\r\n}\r\n\r\n.l-nav ul{\r\n    display: flex;\r\n    justify-content: center;\r\n    list-style-type: none;\r\n    height: 40px;\r\n    background: var(--color-greydarker);\r\n    box-shadow: 0px 2px 5px  rgba(0, 0, 0, .3);\r\n}\r\n\r\n.l-nav ul li{\r\n    padding: 10px;\r\n    width: 100%;\r\n    cursor: pointer;\r\n    text-align: center;\r\n    transition: all .2s ease-in-out;\r\n}\r\n\r\n.l-nav ul li.active, .l-nav ul li:hover{\r\n    box-shadow: 0px -3px 0px var(--color-primary) inset;\r\n}\r\n\r\n.profile-body{\r\n    width: 100%;\r\n    z-index: -1;\r\n}\r\n\r\n.tab{\r\n    padding: 20px;\r\n    width: 100%;\r\n    text-align: center;\r\n    display: none;\r\n}\r\n\r\n#see-more-bio{\r\n    color: var(--color-success);\r\n    cursor: pointer;\r\n    text-transform: lowercase;\r\n}\r\n\r\n#see-less-bio{\r\n    color: var(--color-error);\r\n    cursor: pointer;\r\n    text-transform: lowercase;\r\n}\r\n\r\n@media(max-width:1100px){\r\n    .profile-side{\r\n        width: 250px;\r\n        padding: 90px 15px 20px;\r\n    }\r\n\r\n    .profile-img img{\r\n        height: 200px;\r\n        width: 200px;\r\n        left: 50px;\r\n        top: 50px;\r\n    }\r\n}\r\n\r\n@media(max-width:900px) {\r\n    body{\r\n        margin: 0 20px;\r\n    }\r\n\r\n    .profile-header{\r\n        display: flex;\r\n        height: 100%;\r\n        flex-direction: column;\r\n        text-align: center;\r\n        padding-bottom: 20px;\r\n    }\r\n\r\n    .profile-img{\r\n        float: left;\r\n        width: 100%;\r\n        height: 200px;\r\n    }\r\n\r\n    .profile-img img{\r\n        position: relative;\r\n        left: 0;\r\n    }\r\n\r\n    .profile-nav-info {\r\n        text-align: center;\r\n    }\r\n\r\n    .profile-nav-info .address{\r\n        justify-content: center;\r\n    }\r\n\r\n\r\n    .profile-option{\r\n        right: 20px;\r\n        top: 75%;\r\n        transform: translateY(50%);\r\n    }\r\n\r\n    .main-bd{\r\n        flex-direction: column;\r\n        padding-right: 0px;\r\n    }\r\n\r\n    .profile-side{\r\n        width: 100%;\r\n        padding: 20px;\r\n        text-align: center;\r\n        margin: 5px 0;\r\n    }\r\n\r\n    .user-rating{\r\n        justify-content: center;\r\n    }\r\n\r\n    @media (min-width:400px) {\r\n        body{\r\n            margin: 5px;\r\n        }\r\n    }\r\n\r\n    @media (max-width:400px) {\r\n        body{\r\n            margin: 5px;\r\n        }\r\n    }\r\n}"],"names":[],"version":3,"file":"profile.8848fea8.css.map","sourceRoot":"/__parcel_source_root/"}